{
    "name": "siteorigin/voronoi-placeholder",
    "description": "Creates a Voronoi placeholder that you can use as a placeholder with d3-delaunay.",
    "type": "package",
    "license": "MIT",
    "authors": [
        {
            "name": "Greg Priday",
            "email": "greg@siteorigin.com"
        }
    ],
    "require": {
        "php": "^7.4",
        "ext-imagick": "*",
        "ext-zlib": "*",
        "league/color-extractor" : "^0.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^8"
    },
    "autoload": {
        "psr-4": {
            "SiteOrigin\\VoronoiPlaceholder\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SiteOrigin\\VoronoiPlaceholder\\Tests\\": "tests/"
        }
    }
}
